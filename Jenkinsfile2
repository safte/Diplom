pipeline {
    agent any
    options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
    }
    environment {
      DOCKERHUB_CREDENTIALS = credentials('dockerhub')
    }
    stages {
        stage('Run Makefile') {
            steps {
                sh 'make -f Makefile2'
            }
        }
    }
    post {
      always {
        sh 'docker logout'
      }
  }
}
